      <TextField fx:id="userEmail" layoutX="224.0" layoutY="114.0" prefHeight="34.0" prefWidth="158.0" promptText="Enter your email" />
      <Label layoutX="181.0" layoutY="118.0" prefHeight="26.0" prefWidth="42.0" text="Email:" />
      <Label layoutX="160.0" layoutY="163.0" prefHeight="26.0" prefWidth="64.0" text="Password:" />
      <Label layoutX="168.0" layoutY="289.0" text="Do not have account yet?" />
      <Button layoutX="311.0" layoutY="285.0" mnemonicParsing="false" onAction="#switchToRegistration" prefHeight="18.0" prefWidth="126.0" text="Sing Up!" />
   </children>
</AnchorPane>
